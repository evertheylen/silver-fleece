{
    "compilerOptions": {
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noEmitOnError": true,
        "lib": ["es5", "es6", "dom"],
        "emitDeclarationOnly": true,
        "declaration": true,
        "outDir": "dist"
    },
    "target": "ES5",
    "module": "ES6",
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ],
}
